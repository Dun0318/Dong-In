<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mbc.pdi_05_01.member.MemberService">
<select id="idcheck" resultType="int">select count(*) from member0324 where id=#{param1}</select>
<insert id="memberinsert">insert into member0324 values(#{param1},#{param2},#{param3},#{param4})</insert>
<select id="memberselect" resultType="com.mbc.pdi_05_01.member.MemberDTO">select * from member0324</select>
</mapper>
